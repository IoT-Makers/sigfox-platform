{
  "name": "Dashboard",
  "plural": "Dashboards",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "FireLoop": false,
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "options": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "Widgets": {
      "type": "hasMany",
      "model": "Widget",
      "foreignKey": "dashboardId"
    }
  },
  "acls": [],
  "methods": {}
}
